{
  "name": "artwerk-file-google",
  "version": "1.0.2",
  "description": "Google cloud storage service for Medusa Commerce",
  "author": "artwerk studios UG",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "clean": "cross-env ./node_modules/.bin/rimraf dist",
    "build": "cross-env npm run clean && tsc -p tsconfig.json",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "cross-env tsc --watch",
    "test": "cross-env jest"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-classes": "^7.9.5",
    "@babel/plugin-transform-instanceof": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.5",
    "@babel/preset-typescript": "^7.21.4",
    "@babel/register": "^7.7.4",
    "@babel/runtime": "^7.9.6",
    "@medusajs/medusa-cli": "^1.3.16",
    "@medusajs/types": "^1.11.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/multer": "^1.4.8",
    "@types/node": "^17.0.8",
    "babel-preset-medusa-package": "^1.1.19",
    "client-sessions": "^0.8.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "jest": "^27.3.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "@medusajs/medusa": "^1.9.0",
    "express": "^4.17.2",
    "medusa-interfaces": "1.3.7"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.spec.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/node_modules/"
    ],
    "rootDir": "src",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "transform": {
      ".ts": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
